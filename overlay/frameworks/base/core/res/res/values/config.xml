<!-- These resources are around just to allow their values to be customized
     for different hardware and product builds. -->
<resources>
    <string name="config_datause_iface">wlan0</string>

    <bool name="config_sf_limitedAlpha">true</bool>

    <bool name="config_batterySdCardAccessibility">true</bool>

    <bool name="config_allowAllRotations">true</bool>

    <string-array name="networkAttributes">
        <item>wifi,1,1,3,-1,true</item>
        <item>bluetooth,7,7,2,-1,true</item>
        <item>ethernet,9,9,4,-1,true</item>
        <item>bt_dun,14,14,2,-1,true</item>
        <item>wifi_p2p,13,1,0,-1,true</item>
    </string-array>

    <string-array translatable="false" name="radioAttributes">
        <item>"1,1"</item>
        <item>"7,1"</item>
    </string-array>

   <integer-array translatable="false" name="config_tether_upstream_types">
        <item>1</item>
        <item>7</item>
    </integer-array>

    <bool translatable="false" name="config_wifi_p2p_support">true</bool>

    <bool translatable="false" name="config_wifi_dual_band_support">true</bool>

    <bool name="config_wifi_background_scan_support">true</bool>

    <bool name="config_automatic_brightness_available">true</bool>

    <!-- Array of light sensor LUX values to define our levels for auto backlight brightness support.
         The N entries of this array define N  1 zones as follows:

         Zone 0:        0 <= LUX < array[0]
         Zone 1:        array[0] <= LUX < array[1]
         ...
         Zone N:        array[N - 1] <= LUX < array[N]
         Zone N + 1     array[N] <= LUX < infinity

         Must be overridden in platform specific overlays -->
    <integer-array name="config_autoBrightnessLevels">
        <item>200</item>
        <item>300</item>
        <item>400</item>
        <item>500</item>
        <item>600</item>
        <item>700</item>
        <item>900</item>
        <item>1100</item>
        <item>1400</item>
    </integer-array>

    <!-- Array of output values for LCD backlight corresponding to the LUX values
         in the config_autoBrightnessLevels array.  This array should have size one greater
         than the size of the config_autoBrightnessLevels array.
         This must be overridden in platform specific overlays -->
    <integer-array name="config_autoBrightnessLcdBacklightValues">
        <item>30</item>
        <item>80</item>
        <item>105</item>
        <item>135</item>
        <item>160</item>
        <item>188</item>
        <item>214</item>
        <item>232</item>
        <item>249</item>
        <item>255</item>
    </integer-array>

    <!-- Array of output values for button backlight corresponding to the LUX values
         in the config_autoBrightnessLevels array.  This array should have size one greater
         than the size of the config_autoBrightnessLevels array.
         This must be overridden in platform specific overlays -->
    <integer-array name="config_autoBrightnessButtonBacklightValues">
        <item>0</item>
        <item>255</item>
        <item>255</item>
        <item>255</item>
        <item>255</item>
        <item>255</item>
        <item>255</item>
        <item>255</item>
        <item>255</item>
        <item>255</item>
    </integer-array>

    <!-- Minimum screen brightness allowed by the power manager. -->
    <integer name="config_screenBrightnessDim">4</integer>

    <bool name="config_voice_capable">false</bool>

    <bool name="config_sms_capable">false</bool>

    <integer name="config_downloadDataDirSize">200</integer>

    <string name="config_networkLocationProvider">com.google.android.location.NetworkLocationProvider</string>

    <string name="config_geocodeProvider">com.google.android.location.GeocodeProvider</string>

    <integer name="config_lidOpenRotation">0</integer>

    <integer name="config_lidKeyboardAccessibility">2</integer>

    <bool name="config_screenOffAnimation">false</bool>

    <bool name="config_hasDockBattery">true</bool>

    <string name="config_deviceKeyHandlerLib">/system/framework/com.cyanogenmod.asusec.jar</string>
    <string name="config_deviceKeyHandlerClass">com.cyanogenmod.asusec.KeyHandler</string>
</resources>
